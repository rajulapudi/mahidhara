<section id="products" class="services">
  <div class="container">
    <div class="section-title" data-aos="zoom-out">
      <h1>Products</h1>
      <p>We offer wide range of DERIVATIVES & FINE CHEMICALS.</p>
    </div>


    


<h3 class="productCategory" data-aos="zoom-out">MAIN PRODUCTS</h3>
    <div class="row">
      <% products.forEach(prod => {
        if (prod.category === "main-product") { %>
          <div class="col-lg-4 col-md-6" style="padding-top: 5px;">
            <div class="icon-box" data-aos="zoom-in-left">
              <h4 class="title"><a href="/product/<%- prod.displayUrl %> "><%= (prod.displayName.toUpperCase()) %> </a></h4>
              <p class="description">CAS # <%- prod.cas %> </p>
            </div>
          </div>
      <% } }); %> 
    </div>

