<section id="services" class="services">
  <div class="container">

    <div class="section-title" data-aos="zoom-out">
      <h2>Products</h2>
      <p>We offer wide range of pharmaceutcial grade bulk drugs</p>
    </div>
    <div class="row">
      <% products.forEach(prod => { %>
        <div class="col-lg-4 col-md-6" style="padding-top: 10px;">
          <div class="icon-box" data-aos="zoom-in-left">
            <div class="icon"><img src="<%= prod.image %>" alt="" width="80"></div>
            <h4 class="title"><a href="/product/<%- prod.displayUrl %> "><%- prod.displayName %> </a></h4>
            <p class="description">CAS # <%- prod.cas %> </p>
            <p class="description"><%- prod.name %> </p>
          </div>
        </div>
      <% }) %>
    </div>
  </div>
</section>